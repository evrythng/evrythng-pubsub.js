  ],
  "author": "EVRYTHNG Ltd. <support@evrythng.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/evrythng/evrythng-pubsub.js/issues"
  },
  "homepage": "https://github.com/evrythng/evrythng-pubsub.js#readme",
  "dependencies": {
    "mqtt": "^3.0.0"
  },
  "devDependencies": {
