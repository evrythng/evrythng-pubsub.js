    "@babel/preset-env": "^7.4.5",
    "acorn": "^6.1.1",
    "ajv": "^6.10.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "evrythng": "^5.1.0",
    "mocha": "^6.1.4",
    "standard": "^12.0.1",
    "webpack": "^4.32.2",
